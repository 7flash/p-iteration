{
  "name": "p-iteration",
  "version": "1.1.5",
  "description": "Make array iteration easy when using async/await and Promises",
  "main": "index.js",
  "scripts": {
    "test": "ava test/index.js",
    "lint": "eslint lib test",
    "generate-docs": "jsdoc -c .jsdoc.json -R README.md lib/static-methods.js",
    "update-gh-pages": "git fetch origin gh-pages; git checkout -b docs FETCH_HEAD; git add docs; git commit -m 'Update docs'; git push origin docs:gh-pages; git; checkout master; git branch -D docs",
    "clean-docs": "rm -rf docs"
  },
  "keywords": [
    "async/await",
    "async",
    "await",
    "asynchronous",
    "awaitable",
    "async function",
    "array",
    "iteration",
    "forEach",
    "map",
    "every",
    "some",
    "filter",
    "reduce"
  ],
  "author": "Antonio Valverde",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-eslint": "^7.2.3",
    "eslint": "^4.0.0",
    "jsdoc": "^3.4.3",
    "jsdoc-strip-async-await": "^0.1.0",
    "simple-jsdoc": "0.0.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/toniov/p-iteration"
  }
}
